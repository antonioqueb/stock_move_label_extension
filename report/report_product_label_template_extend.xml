<odoo>
    <record id="view_product_label_layout_form_extend" model="ir.ui.view">
        <field name="name">product.label.layout.form.extend</field>
        <field name="model">product.label.layout.form</field>
        <field name="inherit_id" ref="stock.product_label_layout_form"/>
        <field name="arch" type="xml">
            <!-- Agrega una nueva opción al radio de selección -->
            <xpath expr="//field[@name='print_format']" position="inside">
                <field name="print_format" widget="radio">
                    <option name="new_label">Nuevo Reporte de Etiquetas</option>
                </field>
            </xpath>
        </field>
    </record>
</odoo>
